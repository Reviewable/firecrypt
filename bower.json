{
  "name": "firecrypt",
  "version": "0.1.2",
  "homepage": "https://github.com/pkaminski/firecrypt",
  "authors": [
    "Piotr Kaminski <piotr@ideanest.com>"
  ],
  "description": "Transparent encryption for Firebase",
  "main": "firecrypt.js",
  "keywords": [
    "firebase",
    "encryption"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "firebase": "2.x",
    "serialized-lru-cache": "^3.1.0",
    "cryptojs-extension": "pkaminski/cryptojs-extension#master"
  },
  "overrides": {
    "cryptojs-extension": {
      "main": "build/siv.js"
    },
    "crypto-js": {
      "main": [
        "core.js", "enc-base64.js", "md5.js", "evpkdf.js", "cipher-core.js", "aes.js", "mode-ctr.js"
      ]
    }
  }
}
